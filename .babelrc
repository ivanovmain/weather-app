{
  "presets": [
    [
      "@babel/preset-env",
      {
       "targets": [
         "last 1 chrome versions"
       ],
        "debug": true,
        "corejs": 3,
        "useBuiltIns": "usage",
        "modules": false
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {"legacy": true}
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}